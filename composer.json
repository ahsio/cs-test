{
  "name": "ahsio/cs-test",
  "description": "Provides an API with a CLI delivery mechanism to run mowers and calculate their last positions",
  "type": "library",
  "authors": [
    {
      "name": "Ahmed Siouani",
      "email": "ahmed.siouani@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {"CS\\": "src/"}
  },
  "config": {
    "bin-dir": "bin"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.5"
  },
  "scripts": {
    "build": [
      "@composer install --prefer-dist --optimize-autoloader --ansi --verbose"
    ]
  }
